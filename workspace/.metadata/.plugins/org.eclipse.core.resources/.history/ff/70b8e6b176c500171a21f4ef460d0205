package ProLeague;

import java.io.Serializable;

public class Player implements Serializable{
	
	
	/**
	 * 
	 */
	private static final long serialVersionUID = 7270080219843349751L;
	String name;
	String team;
	String position;
	int age;
	int GamePlayed;
	int mvp;
	
	
	Player(String team,String name,String position,int mvp, int age, int GamePlayed)
	{
		this.name=name;
		this.team=team;
		this.position=position;
		this.age=age;
		this.GamePlayed=GamePlayed;
		this.mvp=mvp;
	}
	
	public String getPlayerName()
	{
		return this.name;
	}
	
	public String getPlayerTeam()
	{
		return this.team;
	}
	public String getPlayerPosition()
	{
		return this.position;
	}
	public int getPlayerAge()
	{
		return this.age;
	}
	public int getPlayerGamePlayed()
	{
		return this.GamePlayed;
	}
	public int getPlayerMVP()
	{
		return this.mvp;
	}
}
